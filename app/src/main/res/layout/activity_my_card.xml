<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent">
    <WebView
        android:id="@+id/web_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/top_bar" />

    <ProgressBar
        android:id="@+id/progress_bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />
    <com.zyjr.emergencylending.custom.TopBar
        android:id="@+id/top_bar"
        app:drawableLeft="@mipmap/icon_return_white"
        app:drawableLeftVisibility="true"
        app:top_background="#00FFFFFF"
        android:layout_marginTop="16dp"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        app:drawableRight="@mipmap/icon_share"
        app:drawableRightVisibility="true"
        android:paddingRight="15dp"
        android:paddingLeft="16dp" />


</FrameLayout>